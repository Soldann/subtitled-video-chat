  "bugs": {
    "url": "https://github.com/Soldann/subtitled-video-chat/issues"
  },
  "homepage": "https://github.com/Soldann/subtitled-video-chat#readme",
  "dependencies": {
    "express": "^4.17.1",
    "socket.io": "^2.3.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "@types/express": "^4.17.8",
    "@types/socket.io": "^2.1.11"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
